

<h1>Welcome to Bar Tom</h1>

<div class="row row-cols-3 row-cols-md-3">
<% @cocktails.each do |cocktail| %>

  <div class="card h-100">
    <div class="card">
       <%= cl_image_tag cocktail.photo.key, height: 280, width: 377, crop: :fill %>
       <div class="card-body">
          <h5 class="card-title"><%= cocktail.name %></h5>
       </div>
    </div>
  </div>

<% end %>
</div>
